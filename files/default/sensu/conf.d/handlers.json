{
	"handlers":{
		"default":{
			"type":"set",
			"handlers":["gelf"]
		},
		"event":{
			"type": "pipe",
			"command": "/etc/sensu/handlers/system_event_handler.rb"
		},
		"gelf": {
		     "type": "pipe",
		     "command": "/etc/sensu/handlers/gelf.rb"
		},
		"graphite":{
			"type":"amqp",
			"exchange":{
				"type":"topic",
				"name":"graphite",
				"passive":"true"
				},
			"send_only_check_output":true
		}
	}
}
